<table class="table">
  <thead>
  <tr>
    <th>Conteúdo</th>
    <th>Fonte</th>
    <th>Ano</th>
    <th>Usuário</th>
    <th colspan="3"></th>
  </tr>
  </thead>
  <tbody>
  <% questions.each do |question| %>
    <tr>
      <td><%= question.content %></td>
      <td><%= question.source %></td>
      <td><%= question.year %></td>
      <td><%= question.user.email %></td>
      <td><%= link_to 'Revisar', edit_admin_question_path(question), class: 'btn btn-primary' %></td>
    </tr>
  <% end %>
  </tbody>
</table>


